        <attr name="noEnableRadius" format="dimension"/>
        <attr name="noEnableLeftPadding" format="dimension"/>
        <attr name="noEnableTopPadding" format="dimension"/>
        <attr name="noEnableRightPadding" format="dimension"/>
        <attr name="noEnableBottomPadding" format="dimension"/>
        <attr name="noEnablePadding" format="dimension"/>
        <attr name="noEnableColor" format="color|reference"/>
        <attr name="noEnableStrokeColor" format="color|reference"/>
        <attr name="noEnableStrokeWidth" format="dimension"/>
        <attr name="noEnableDashWidth" format="dimension"/>
        <attr name="noEnableDashGap" format="dimension"/>
    </declare-styleable>
</resources>